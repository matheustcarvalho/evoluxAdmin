        <h5 class="modal-title" id="exampleModalLabel">Atenção!</h5>
        <button class="close" type="button" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">×</span>
        </button>
    </div>
    <div class="modal-body">Clique em "Baixar" para dar baixa nos registros selecionados.</div>
    <div class="modal-footer">
        <button class="btn btn-secondary" type="button" data-dismiss="modal">Cancelar</button>
        <a class="btn btn-success" href="login.html">Baixar</a>
    </div>
</div>
</div>
</div>

<script>
$('#formReceber').on("submit", function (e) {

e.preventDefault();
let formData = new FormData($("#formReceber").get(0));

const data = {};
formData.forEach((val, key) => {
    data[key] = val;
});

$.ajax({
    url: '/nova-conta-receber',
    method: 'POST',
    dataType: 'json',
    data,
    success: function (response) {
        alert('Salvo com sucesso!')
    }, error: function (e) {
        console.log(e);
    }

});
});
</script>

